      <interpreter home="/usr/bin/php" id="1985ff78-8a4e-4a72-8ac8-1d7963524307" name="PHP 7" debugger_id="php.debugger.XDebug" />
    </interpreters>
  </component>
  <component name="PhpSmartKeysConfiguration">
