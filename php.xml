    <phpcs_settings>
      <PhpCSConfiguration max_messages_per_file="100" standards="MySource;PEAR;PHPCS;PSR1;PSR2;Squiz;Zend" tool_path="$USER_HOME$/web/prezly/vendor/bin/phpcs" />
